{
  "name": "auto-package-updater",
  "version": "0.1.0",
  "description": "Automatically update the packages in your projects",
  "bin": {
    "auto-package-updater": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc && chmod +x dist/index.js",
    "dev": "tsc -w",
    "test": "tsc --noEmit && biome check . && vitest run src"
