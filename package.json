  "license": "ISC",
  "packageManager": "pnpm@10.20.0",
  "dependencies": {
    "@inquirer/prompts": "^8.1.0",
    "chalk": "^5.6.2",
    "command-line-args": "^6.0.1",
    "command-line-usage": "^7.0.3",
    "execa": "^9.6.1",
    "semver": "7.7.3",
    "vitest": "4.0.16"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@types/command-line-args": "^5.2.3",
    "@types/command-line-usage": "^5.0.4",
    "@types/node": "^25.0.3",
    "@types/semver": "^7.7.1",
    "typescript": "^5.9.3"
  }
